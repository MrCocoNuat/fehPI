{
    "git.branchProtection": [
        "main"
    ],
    "git.branchProtectionPrompt": "alwaysCommitToNewBranch",

    "editor.quickSuggestions": {
        "other": "on",
        "comments": "off",
        "strings": "on"
    },

    "files.associations": {
        "*.css": "tailwindcss"
    },
    "conventionalCommits.scopes": [
        "battle engine",
        "explorer"
    ]
}